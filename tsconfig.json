{
  "compilerOptions": {
    
    "target": "es2016",                                  
    "lib": ["DOM", "DOM.Iterable", "ESNext"],                                       
    "jsx": "react-jsx",                                
    "module": "commonjs",                                
    "moduleResolution": "node",                       
    "resolveJsonModule": true,                        
    "allowJs": true,                                  
    "noEmit": true,                                  
    "isolatedModules": true,                          
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                      
    "skipLibCheck": true                                 
  },
  "include": ["src"]
}
